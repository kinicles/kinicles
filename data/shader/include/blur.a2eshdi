<?xml version="1.0" encoding="UTF-8"?>
<a2e_shader_include version="1">
	<vertex_shader>
		<variables>
in vec2 in_vertex;
in vec2 in_tex_coord;
out vec2 tex_coord;
		</variables>
		<program>
void main() {
	gl_Position = vec4(in_vertex.x, in_vertex.y, 0.0, 1.0);
	tex_coord = in_tex_coord;
}
		</program>
	</vertex_shader>
</a2e_shader_include>